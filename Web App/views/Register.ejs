<%- include('header', {siteTitle: siteTitle, appTitle: appTitle})%>

<div>
    <form class="authForm">
        <div class="formField">
            <label for="email">Enter Email:</label>
            <input type="email" id="email" max="100">
        </div>
        <div class="formField">
            <label for="username">Enter Username:</label>
            <input type="text" id="username" max="100">
        </div>
        <div class="formField">
            <label for="password">Enter password:</label>
            <input type="password" id="password" min="8" max="30">
        </div>
        <div class="formField">
            <label for="repeat-password">Renter password:</label>
            <input type="password" id="repeat-password" min="8" max="30">
        </div>
    </form>
</div>

<%- include('footer') %>